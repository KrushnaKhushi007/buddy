{
  "name": "buddy-plugin-coffeescript",
  "description": "Buddy plugin for coffee-script source transforms (via Coffee-script)",
  "version": "1.11.0",
  "author": "popeindustries <alex@pope-industries.com>",
  "keywords": [
    "buddy",
    "compile",
    "coffeescript",
    "coffee-script"
  ],
  "dependencies": {
  },
